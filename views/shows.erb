<div class="mostPopularContainer collapse">
	<ul class="mostPopular">
    <% @popular_shows.each do |show| %>
	    <li>
	    	<a href="/show/<%= show.id %>" style="background-image: url(<%= show.poster_path %>)" alt="<%= show.title %>" title="<%= show.title %>"><%= show.title %></a>
	    </li>
    <% end %>
	</ul>
</div>

<div class="clear"></div>

<div class="mod-bar">
<div class="bar-inline bar-genres">
<ul>
	{genres}
	<li><a href="javascript:;" class="btn-more-genres{genreFilterActive}">Genres...</a></li>
</ul>
<div class="clear"></div>
</div>

</div>

<ul id="more-genres" class="genre-dropdown">
	{otherGenres}
</ul>

<select name="genres" id="genreSelector">{selectGenres}</select>

<div class="unit1of3">

<div class="box">
<h4>Upcoming premieres <span>Are you ready?</span></h4>

<ul class="boxList showList premieres">
	{showPremieres}
</ul>

<div class="clear"></div>
</div>

</div>

<div class="unit1of3">

<div class="box">
<h4>Popular <span>Looking for something new?</span></h4>

<ul class="boxList showList popular">
	{popular}
</ul>

<div class="clear"></div>

</div>

</div>

<div class="unit1of3">

<div class="box last-item">
<h4>Recently aired <span>Get seated. Now.</span></h4>

<ul class="boxList showList episodes">
	{recentEpisodes}
</ul>

<div class="clear"></div>

</div>

</div>

<div class="clear"></div>

<div id="allShows">
{allShows}
</div>
